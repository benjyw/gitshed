{
  "content_store_local_example": {
    "local": {
      "root": "/data/local/gitshed"
    }
  },
  "content_store_remote_example": {
    "remote": {
      "host": "bodega",
      "root_path": "/data/appdata/bodega/gitshed/foursquare.web",
      "root_url": "http://bodega/gitshed/foursquare.web/",
      "timeout_secs": 10
    }
  },

  "exclude": [".pants.d", ".pants.bootstrap"],

  "concurrency": {
    "get": 12,
    "put": 6
  }
}
